@model IEnumerable<CS481_Hub.Models.Available_API>

@{
    ViewBag.Title = "Update";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Delete</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayName("API Name")
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.API_Name)
        </td>
        <td>
            @Html.ActionLink("Delete", "DeleteAsync", new { id=item.API_ID }, new { @style = "text-decoration: none;" })
        </td>
    </tr>
}

</table>
